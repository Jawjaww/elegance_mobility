# Project URL (required)
# Find in: Project Settings > API > Project URL
NEXT_PUBLIC_SUPABASE_URL=your_project_url

# Service Role key (required for admin operations)
# ⚠️ WARNING: This key has full admin access. Keep it secure.
# Find in: Project Settings > API > service_role key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Anon key (required for normal operations)
# Find in: Project Settings > API > anon/public key
SUPABASE_ANON_KEY=your_anon_key
